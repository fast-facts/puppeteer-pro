{
  "name": "puppeteer-pro",
  "version": "1.5.0",
  "description": "A simple puppeteer wrapper to enable useful plugins with ease",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "postbuild": "npm run copy:injections && npm run copy:files",
    "copy:injections": "copyfiles -u 1 src/plugins/*/injections/* dist",
    "copy:files": "copyfiles package.json README.md LICENSE dist",
    "test": "mocha --timeout 30000"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItzRabbs/puppeteer-pro.git"
  },
  "author": "ItzRabbs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItzRabbs/puppeteer-pro/issues"
  },
  "homepage": "https://github.com/ItzRabbs/puppeteer-pro#readme",
  "keywords": [
    "puppeteer",
    "chrome",
    "automation",
    "plugin",
    "anonymize",
    "detection",
    "stealth"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "ghost-cursor": "^1.1.6",
    "user-agents": "^1.0.700"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "@types/user-agents": "^1.0.1",
    "chai": "^4.3.4",
    "copyfiles": "^2.2.0",
    "dotenv-safe": "^8.2.0",
    "eslint": "^8.0.0",
    "mocha": "^9.0.1",
    "puppeteer": "^13.0.0",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.1",
    "typescript": "^4.0.2"
  }
}
